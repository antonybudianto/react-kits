{
  "name": "~~name~~",
  "version": "0.7.4",
  "description": "react ssr starter",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "better-npm-run start",
    "clean": "react-kits clean",
    "lint": "react-kits lint",
    "test": "jest",
    "build": "npm run deploy:prod",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:staging": "better-npm-run deploy:staging",
    "deploy:prod": "better-npm-run deploy:prod",
    "serve": "better-npm-run serve",
    "prettier": "prettier --write \"{src,config}/**/*.js\""
  },
  "betterScripts": {
    "start": {
      "command": "react-kits start",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "serve": {
      "command": "node dist/bundle.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "deploy:dev": {
      "command": "react-kits build",
      "env": {
        "NODE_ENV": "development",
        "APP_ENV": "development"
      }
    },
    "deploy:staging": {
      "command": "react-kits build",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "staging"
      }
    },
    "deploy:prod": {
      "command": "react-kits build",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "production"
      }
    }
  },
  "author": "User",
  "jest": {
    "globals": {
      "__DEV__": false,
      "__TEST__": true,
      "__PROD__": false,
      "__STAG__": false
    }
  },
  "dependencies": {
    "axios": "0.18.0",
    "babel-polyfill": "^6.26.0",
    "basscss": "^8.0.4",
    "better-npm-run": "^0.1.0",
    "classnames": "^2.2.5",
    "dotenv": "6.0.0",
    "loadable-components": "^2.2.2",
    "lodash": "4.17.10",
    "lodash-es": "4.17.10",
    "npm-run-all": "4.1.3",
    "prop-types": "^15.6.1",
    "query-string": "^6.1.0",
    "raf": "3.4.0",
    "react": "16.4.1",
    "react-body-classname": "^1.2.0",
    "react-dom": "16.4.1",
    "react-helmet": "5.2.0",
    "react-hot-loader": "^4.3.3",
    "react-kits-server": "^0.7.4",
    "react-redux": "5.0.7",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "recompose": "0.27.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "ua-parser-js": "0.7.18"
  },
  "devDependencies": {
    "http-proxy-middleware": "^0.18.0",
    "jest": "^23.1.0",
    "prettier": "1.13.7",
    "react-kits": "^0.7.4",
    "webpack-hot-middleware": "^2.22.2"
  }
}
