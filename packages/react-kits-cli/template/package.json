{
  "name": "~~name~~",
  "version": "3.0.0-alpha.3",
  "description": "react ssr starter",
  "private": true,
  "scripts": {
    "start": "bnr start",
    "clean": "react-kits clean",
    "lint": "react-kits lint",
    "dll": "react-kits build-dll",
    "test": "jest",
    "build": "npm run build:prod",
    "build:dev": "bnr build:dev",
    "build:staging": "bnr build:staging",
    "build:prod": "bnr build:prod",
    "serve": "bnr serve",
    "prettier": "prettier --write \"src/**/*.js\""
  },
  "betterScripts": {
    "start": {
      "command": "react-kits start",
      "env": {
        "NODE_ENV": "development",
        "APP_ENV": "development",
        "APP_ASSET_PATH": "/myapp/"
      }
    },
    "serve": {
      "command": "node dist/bundle.js",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "production"
      }
    },
    "build:dev": {
      "command": "react-kits build",
      "env": {
        "NODE_ENV": "development",
        "APP_ENV": "development"
      }
    },
    "build:staging": {
      "command": "react-kits build",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "staging",
        "APP_ASSET_PATH": "/myapp/"
      }
    },
    "build:prod": {
      "command": "react-kits build",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "production",
        "APP_ASSET_PATH": "/myapp/"
      }
    }
  },
  "author": "User",
  "browserslist": [
    "> 1% in ID",
    "last 2 versions",
    "Firefox ESR",
    "iOS >= 9",
    "not ie < 11"
  ],
  "jest": {
    "testURL": "http://localhost",
    "globals": {
      "__DEV__": false,
      "__TEST__": true,
      "__PROD__": false,
      "__STAG__": false
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "react-ua": "0.2.0",
    "axios": "0.18.0",
    "basscss": "8.0.4",
    "better-npm-run": "^0.1.1",
    "classnames": "^2.2.6",
    "dotenv": "6.2.0",
    "loadable-components": "2.2.3",
    "lodash": "4.17.11",
    "lodash-es": "4.17.11",
    "npm-run-all": "4.1.5",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "raf": "3.4.1",
    "react": "16.6.3",
    "react-body-classname": "^1.2.0",
    "react-dom": "16.6.3",
    "react-helmet-async": "0.1.0",
    "react-hot-loader": "^4.3.12",
    "react-kits-server": "^3.0.0-alpha.3",
    "react-redux": "5.0.7",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "recompose": "0.30.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "http-proxy-middleware": "^0.19.1",
    "jest": "23.6.0",
    "prettier": "1.15.3",
    "react-kits": "^3.0.0-alpha.3",
    "webpack-hot-middleware": "^2.24.3"
  }
}
