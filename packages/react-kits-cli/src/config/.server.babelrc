{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "babel-plugin-dynamic-import-node",
    "@babel/plugin-proposal-class-properties",
    "loadable-components/babel"
  ]
}
